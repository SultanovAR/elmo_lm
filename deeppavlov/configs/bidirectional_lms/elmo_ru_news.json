{
  "chainer": {
    "in": [
      "sentences"
    ],
    "pipe": [
      {
        "in": ["sentences"],
        "name": "lazy_tokenizer",
        "out": ["tokens"]
      },
      {
        "name": "elmo_bilm",
        "mini_batch_size": 32,
        "in": [
          "tokens"
        ],
        "model_dir": "bidirectional_lms/elmo_ru_news",
        "out": [
          "pred_tokens"
        ]
      }
    ],
    "out": [
      "pred_tokens"
    ]
  },
  "metadata": {
    "requirements": [
      "../dp_requirements/tf.txt",
      "../dp_requirements/elmo.txt"
    ],
    "download": [
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/lm_elmo_ru_news.tar.gz",
        "subdir": "bidirectional_lms/"
      }
    ]
  }
}